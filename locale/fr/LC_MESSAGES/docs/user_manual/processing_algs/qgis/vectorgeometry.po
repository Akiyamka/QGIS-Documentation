msgstr "Largeur de la forme du buffer"

#: ../../docs/user_manual/processing_algs/qgis/vectorgeometry.rst:5222
msgid "**Height**"
msgstr "**Hauteur**"

#: ../../docs/user_manual/processing_algs/qgis/vectorgeometry.rst:5223
msgid "``HEIGHT``"
msgstr "``HEIGHT``"

#: ../../docs/user_manual/processing_algs/qgis/vectorgeometry.rst:5227
msgid "Height of the buffer shape"
msgstr "Hauteur de la forme du buffer"

#: ../../docs/user_manual/processing_algs/qgis/vectorgeometry.rst:5228
msgid "**Rotation**"
msgstr "**Rotation**"

#: ../../docs/user_manual/processing_algs/qgis/vectorgeometry.rst:5231
msgid "``ROTATION``"
msgstr "``ROTATION``"

#: ../../docs/user_manual/processing_algs/qgis/vectorgeometry.rst:5235
msgid "Rotation of the buffer shape"
msgstr "Rotation de la forme du buffer"

#: ../../docs/user_manual/processing_algs/qgis/vectorgeometry.rst:5236
msgid "**Number of segment**"
msgstr "**Nombre de segments**"

#: ../../docs/user_manual/processing_algs/qgis/vectorgeometry.rst:5240
