msgstr ""
"Devuelve la representación Well-Known Text (WKT) de una geoemtría sin "
"metadatos SRID."

#: ../../docs/user_manual/working_with_vector/expression_help/GeometryGroup.rst:776
msgid "geom_to_wkt(geometry, [precision=8])"
msgstr "geom_to_wkt(geometry, [precision=8])"

#: ../../docs/user_manual/working_with_vector/expression_help/GeometryGroup.rst:781
msgid "**precision** - numeric precision"
msgstr "**precision** - precisión numérica"

#: ../../docs/user_manual/working_with_vector/expression_help/GeometryGroup.rst:783
msgid "``geom_to_wkt( make_point(6, 50) )`` → 'POINT(6 50)'"
msgstr "``geom_to_wkt( make_point(6, 50) )`` → 'POINT(6 50)'"

