#: ../../docs/user_manual/processing_algs/taudem/basic_grid_analysis_tools.rst:475
msgid "6 --- Southwest"
msgstr "6 --- Sud Ovest"

#: ../../docs/user_manual/processing_algs/taudem/basic_grid_analysis_tools.rst:476
msgid "7 --- South"
msgstr "7 --- Sud"

